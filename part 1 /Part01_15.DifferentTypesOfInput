
import java.util.Scanner;

public class DifferentTypesOfInput {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);

        System.out.println("Give a string:");
        String ans1 =scan.nextLine(); 
        System.out.println("Give an integer:");
        int ans2 = Integer.valueOf(scan.nextLine());
        
        System.out.println("Give a double:");
        double ans3 = Double.valueOf(scan.nextLine());
        
        System.out.println("Give a boolean:");
        boolean ans4 = Boolean.valueOf(scan.nextLine());
        
        System.out.println("You gave the string "+ans1);
        System.out.println("You gave the integer "+ans2);
        System.out.println("You gave the double "+ans3);
        System.out.println("You gave the boolean "+ans4);

    }
}
